projection;
strict ( 2 );

define behavior for ZRD_PV_HEADER alias Header
{
  use create;
  use update;
  use delete;

  use action acceptTravel;
  use action rejectTravel;

  use association Item { create; }
}

define behavior for ZRD_PV_ITEM //alias <alias_name>
{
  use update;
  use delete;

  use association Header;
}